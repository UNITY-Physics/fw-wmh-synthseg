{
  "author": "Niall Bourke",
  "cite": "Z\u00f6llei, L, Iglesias, JE, Ou, Y, Grant, PR, Fischl,B. Infant FreeSurfer: An automated segmentation and surface extraction pipeline for T1-weighted neuroimaging data of infants 0-2 years, NeuroImage, 2020, 116946, https://doi.org/10.1016/j.neuroimage.2020.116946.",
  "command": "python3 /flywheel/v0/run.py",
  "config": {
    "age": {
      "default": null,
      "description": "Age of the infant at the time of scan (in months). If set to 0, the age will be taken from the subject metadata.",
      "maximum": 12,
      "minimum": 1
    }
  },
  "custom": {
    "analysis-level": "participant",
    "flywheel": {
      "classification": {
        "components": [
          "FreeSurfer"
        ],
        "keywords": [
          "segmentation",
          "volume",
          "template"
        ],
        "modality": [
          "MR"
        ],
        "type": [
          "nifti"
        ]
      },
      "suite": "UNITY Apps"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/infant-freesurfer:1.0.1"
    },
    "kcl-app-binary": "app/main.sh",
    "kcl-app-modalities": [
      "anat"
    ],
    "license": {
      "dependencies": [
        {
          "name": "Other",
          "url": "/usr/local/freesurfer/license.txt"
        }
      ],
      "main": {
        "name": "",
        "url": ""
      },
      "non-commercial-use-only": false
    }
  },
  "description": "Infant FreeSurfer ",
  "environment": {
    "CONDA_DIR": "/opt/conda",
    "FLYWHEEL": "/flywheel/v0",
    "GPG_KEY": "E3FF2839C048B25C084DEBE9B26995E310250568",
    "LANG": "C.UTF-8",
    "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "PYTHON_GET_PIP_SHA256": "fa6f3fb93cce234cd4e8dd2beb54a51ab9c247653b52855a48dd44e6b21ff28b",
    "PYTHON_GET_PIP_URL": "https://github.com/pypa/get-pip/raw/c20b0cfd643cd4a19246ccf204e2997af70f6b21/public/get-pip.py",
    "PYTHON_PIP_VERSION": "21.2.3",
    "PYTHON_VERSION": "3.8.11",
    "FREESURFER": "/usr/local/freesurfer",
    "FREESURFER_HOME": "/usr/local/freesurfer/8.1.0",
    "container": "oci",
    "_": "/usr/bin/printenv",
    "SHLVL": "0",
    "PWD": "/flywheel/v0",
    "OS": "Linux",
    "SUBJECTS_DIR": "/usr/local/freesurfer/subjects",
    "MNI_DIR": "/usr/local/freesurfer/mni",
    "FSFAST_HOME": "/usr/local/freesurfer/fsfast",
    "FSF_OUTPUT_FORMAT": "nii.gz",
    "LOCAL_DIR": "/usr/local/freesurfer/local",
    "FMRI_ANALYSIS_DIR": "/usr/local/freesurfer/fsfast",
    "FUNCTIONALS_DIR": "/usr/local/freesurfer/sessions",
    "MNI_PERL5LIB": "/usr/local/freesurfer/mni/share/perl5",
    "MINC_BIN_DIR": "/usr/local/freesurfer/mni/bin",
    "PERL5LIB": "/usr/local/freesurfer/mni/share/perl5",
    "FS_OVERRIDE": "0",
    "MINC_LIB_DIR": "/usr/local/freesurfer/mni/lib",
    "MNI_DATAPATH": "/usr/local/freesurfer/mni/data",
    "FSLDIR": "/opt/conda",
    "PYTHONPATH": "/flywheel/v0:"
  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "input file (usually isotropic reconstruction)",
      "optional": false
    }
  },
  "label": "infant-freesurfer",
  "license": "MIT",
  "maintainer": "Niall Bourke <niall.bourke@kcl.ac.uk>",
  "name": "infant-freesurfer",
  "source": "https://surfer.nmr.mgh.harvard.edu/fswiki/infantFS",
  "url": "https://github.com/UNITY-Physics/fw-infant-freesurfer",
  "version": "1.0.1"
}