{
  "author": "Niall Bourke, Annika Robiolio",
  "cite": "Z\u00f6llei, L, Iglesias, JE, Ou, Y, Grant, PR, Fischl,B. Infant FreeSurfer: An automated segmentation and surface extraction pipeline for T1-weighted neuroimaging data of infants 0-2 years, NeuroImage, 2020, 116946, https://doi.org/10.1016/j.neuroimage.2020.116946.",
  "command": "python3 /flywheel/v0/run.py",
  "config": {},
  "custom": {
    "analysis-level": "participant",
    "flywheel": {
      "classification": {
        "components": [
          "FreeSurfer"
        ],
        "keywords": [
          "segmentation",
          "volume",
          "template"
        ],
        "modality": [
          "MR"
        ],
        "type": [
          "nifti"
        ]
      },
      "suite": "UNITY Apps"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/wmh-synthseg:0.0.6"
    },
    "kcl-app-binary": "app/main.sh",
    "kcl-app-modalities": [
      "anat"
    ],
    "license": {
      "dependencies": [
        {
          "name": "Other",
          "url": "/usr/local/freesurfer/license.txt"
        }
      ],
      "main": {
        "name": "",
        "url": ""
      },
      "non-commercial-use-only": false
    }
  },
  "description": "WMH-SynthSeg",
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "input file (usually isotropic reconstruction)",
      "optional": false
    }
  },
  "label": "WMH-SynthSeg",
  "license": "MIT",
  "maintainer": "Niall Bourke <niall.bourke@kcl.ac.uk>",
  "name": "wmh-synthseg",
  "source": "https://surfer.nmr.mgh.harvard.edu/fswiki/WMH-SynthSeg",
  "url": "https://github.com/UNITY-Physics/fw-WMH-SynthSeg",
  "version": "0.0.6",
  "environment": {
    "FREESURFER_HOME": "/usr/local/freesurfer/8.1.0",
    "FLYWHEEL": "/flywheel/v0",
    "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "PWD": "/flywheel/v0"
  }
}